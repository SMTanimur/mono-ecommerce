  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "importOrder": [
    "^@testing-library/jest-dom$",
    "^@testing-library(.*)|react-test-renderer$",
    "^react|@storybook/react$",
